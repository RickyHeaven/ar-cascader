{
  "name": "ar-cascader",
  "version": "1.1.0",
  "description": "regionalism cascader base on view-design，中国行政区域区域划分(含港澳台)级联选择器，三级，省-市-区，数据来源于国家统计局2023年6月统计用区划代码",
  "files": [
    "lib",
    "src",
    "types"
  ],
  "main": "/lib/ar-cascader.umd.min.js",
  "module": "./lib/ar-cascader.umd.min.js",
  "typings": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/ar-cascader.umd.min.js",
      "require": "./lib/ar-cascader.common.js",
      "types": "./types/index.d.ts"
    },
    "./lib/**/*.js": "./lib/**/*.js",
    "./*": "./*"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lib": "vue-cli-service build --target lib --name ar-cascader --dest lib src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/ar-cascader.git"
  },
  "keywords": [
    "administrative",
    "region",
    "cascader",
    "ar",
    "ricky",
    "zhangqingcq",
    "vue",
    "view-design",
    "area-data",
    "China",
    "行政区域代码",
    "中国省份",
    "行政区域级联"
  ],
  "author": "RickyHeaven",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RickyHeaven/ar-cascader/issues"
  },
  "homepage": "https://github.com/RickyHeaven/ar-cascader#readme",
  "dependencies": {
    "@zhangqingcq/china-area-data": "^0.0.3",
    "core-js": "^3.8.3",
    "lodash-es": "^4.17.21",
    "view-design": "^4.7.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.4"
  }
}
